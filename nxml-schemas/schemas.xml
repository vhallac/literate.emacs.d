  <documentElement prefix="" localName="Ui" typeId="wowui"/>

  <namespace ns="http://maven.apache.org/POM/4.0.0" uri="maven-v4_0_0.rnc"/>

  <typeId id="wowui" uri="wowui.rnc"/>
  <typeId id="html5" uri="html5.rnc"/>
  <typeId id="xhtml5" uri="xhtml5.rnc"/>
  <typeId id="spring-beans" uri="spring-beans.rnc"/>
  <typeId id="struts" uri="struts-config_1.2.rnc"/>
